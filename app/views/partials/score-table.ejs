<% if (standing.length > 1) { %>
<div>
    <table>
        <tr>
            <th><%=__('team-label')%></th>
            <th><%=__('points-label')%></th>
        </tr>

        <% for(var i=0; i < standing.length; i++) { %>
        <tr>
            <td data-test="name-<%=i%>"><%=__(`team-${standing[i].name}`)%></td>
            <td data-test="score-<%=i%>" class="center"><%= standing[i].score %></td>
        </tr>
        <% } %>
    </table>
</div>
<% } else { %>
<p class="center"><%=__('waiting-text')%></p>
<% } %>



